#!/usr/bin/expect

set timeout -1

cd /var/www/html
spawn php setup/setup.php install /var/www/config/minimal-config.json

expect {
    -re ".*Are you fine with this.*" {
        send "yes\r"
        exp_continue
    }
    -re ".*If you still proceed.*" {
        send "yes\r"
        exp_continue
    }
    -re ".*Also.*" {
        send "yes\r"
        exp_continue
    }

}

expect eof
